      <Package id="_1lEe" name="tree">
        <Procedure id="_1lDm" name="doubleclick_product_node"
                   postdelay="1000">
          <variable name="prod_id"/>
          <variable name="band"/>
          <variable name="group_id"/>
          <variable name="view_id"/>
          <comment>@teststep Doubleclick on the node in the product tree</comment>
          <ProcedureCall id="_1oRr" procedure="get.bands_node_id">
            <variable name="prod_id">$(prod_id)</variable>
          </ProcedureCall>
          <IfSequence id="_1oTc"
                      test="&#34;$(bands_group)&#34;==&#34;true&#34;">
            <SetGlobalStep id="_1oUQ" varname="nid">
              <default>/$(prod_id)/$(bands_node_id)/$(view_id)</default>
            </SetGlobalStep>
            <IfSequence disabled="true" id="_1oQJ"
                        test="&#34;$(group_id)&#34;==&#34;&#34;">
              <SetGlobalStep id="_1oXH" varname="nid">
                <default>/$(prod_id)/$(bands_node_id)/$(view_id)</default>
              </SetGlobalStep>
              <ElseSequence id="_1oQL">
                <SetGlobalStep id="_1oXI" varname="nid">
                  <default>/$(prod_id)/$(bands_node_id)/$(group_id)/$(view_id)</default>
                </SetGlobalStep>
              </ElseSequence>
            </IfSequence>
            <ElseSequence id="_1oTd">
              <IfSequence id="_1oUN"
                          test="&#34;$(band)&#34;&lt;&gt;&#34;&#34;">
                <ProcedureCall id="_1oUM" procedure="get.get_index_of_node">
                  <variable name="node">$(band)</variable>
                  <variable name="nodeSeparator">@</variable>
                  <variable name="linear">false</variable>
                </ProcedureCall>
                <ProcedureCall disabled="true" id="_1oTr"
                               procedure="get.band_id">
                  <variable name="prod_id">$(prod_id)</variable>
                  <variable name="band">$(band)</variable>
                </ProcedureCall>
                <SetGlobalStep id="_1oUT" varname="nid">
                  <default>$(new_band_id)</default>
                </SetGlobalStep>
                <ElseSequence id="_1oUO">
                  <SetGlobalStep id="_1oUP" varname="nid">
                    <default>/$(prod_id)/0</default>
                  </SetGlobalStep>
                </ElseSequence>
              </IfSequence>
            </ElseSequence>
          </IfSequence>
          <MouseEventStep clicks="2" client="$(client)"
                          component="NbMainWindow.Product_Explorer.tree&amp;$(nid)"
                          event="MOUSE_MPRC" id="_1oTi" modifiers="16"/>
        </Procedure>
        <Procedure disabled="true" id="_1mQM" name="layer_checkbox">
          <variable name="layer_node1"/>
          <variable name="layer_node2"/>
          <IfSequence id="_1mQN" test="&#34;$(layer_node2)&#34;==&#34;&#34;">
            <MouseEventStep clicks="1" client="$(client)"
                            component="Dialog.LayerManagerToolView.Control.treeTransparency:&amp;/$(layer_node1)"
                            event="MOUSE_MPRC" id="_1mJp" modifiers="16"/>
            <ElseSequence id="_1mQP">
              <MouseEventStep clicks="1" client="$(client)"
                              component="Dialog.LayerManagerToolView.Control.treeTransparency:&amp;/$(layer_node1)/$(layer_node2)"
                              event="MOUSE_MPRC" id="_1mQS" modifiers="16"/>
            </ElseSequence>
          </IfSequence>
        </Procedure>
        <Procedure disabled="true" id="_1mQT" name="layer_node">
          <variable name="layer_node1"/>
          <variable name="layer_node2"/>
          <IfSequence id="_1mQU" test="&#34;$(layer_node2)&#34;==&#34;&#34;">
            <MouseEventStep clicks="1" client="$(client)"
                            component="Dialog.LayerManagerToolView.Control.treeTransparency:&amp;/$(layer_node1)"
                            event="MOUSE_MPRC" id="_1mJv" modifiers="16"
                            x="-12" y="8"/>
            <ElseSequence id="_1mQW">
              <MouseEventStep clicks="1" client="$(client)"
                              component="Dialog.LayerManagerToolView.Control.treeTransparency:&amp;/$(layer_node1)/$(layer_node2)"
                              event="MOUSE_MPRC" id="_1mQY" modifiers="16"
                              x="-12" y="8"/>
            </ElseSequence>
          </IfSequence>
        </Procedure>
